{
  "name": "hello-world-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "start:en": "webpack-dev-server --config ./webpack/webpack.en.js",
    "start:nl": "webpack-dev-server --config ./webpack/webpack.nl.js",
    "build": "npm run build:en && npm run build:nl",
    "build:en": "webpack --config ./webpack/webpack.en.js",
    "build:nl": "webpack --config ./webpack/webpack.nl.js",
    "watch:test": "nodemon ./loaders/i18n/sprintfToJsx.spec.js"
  },
  "dependencies": {
    "html-webpack-plugin": "^2.26.0",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "loader-utils": "^1.1.0",
    "nodemon": "^1.12.1",
    "parse5": "^3.0.2",
    "webpack-merge": "^4.1.0"
  }
}
